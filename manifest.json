{
    "name": "LinkedIn Navigation",
    "version": "1.1",
    "manifest_version": 2,
    
    "description": "Hardcoded Website DOM Navigation.",
    "icons":{
        "64": "inactive_face.jpg"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
            "activeTab",
            "storage"
    ]
}